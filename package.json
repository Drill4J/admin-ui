{
  "name": "drill4j",
  "version": "0.4.0",
  "license": "Apache-2.0",
  "private": true,
  "dependencies": {
    "@drill4j/socket": "0.1.4",
    "@redneckz/react-bem-helper": "2.0.4",
    "@redneckz/react-devirt": "0.0.6",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.14",
    "@types/react": "16.9.13",
