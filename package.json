    ],
    "*.{sass,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintIgnore": [
    "*.spec.ts",
    "*.spec.tsx",
    "serviceWorker.ts",
    "react-app-env.d.ts"
  ]
